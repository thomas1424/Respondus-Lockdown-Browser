{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LockDown Browser",
  "version": "0.4.38.1",
  "author": "VERSIONPROD",
  "default_locale": "en",
  "description": "LockDown Browser is a custom browser that locks down the exam environment within specific learning and assessment systems",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "assets/images/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/images/icon.png"
    }
  },
  "incognito": "not_allowed",
  "permissions": [
    "system.display",
    "history",
    "cookies",
    "storage",
    "webNavigation",
    "webRequest",
    "tabs",
    "activeTab",
    "scripting",
    "management",
    "contentSettings",
    "browsingData",
    "declarativeNetRequestWithHostAccess",
    "clipboardRead",
    "clipboardWrite",
    "wallpaper",
    "alarms"
  ],
  "content_scripts": [],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "background.js",
        "manifest.json",
        "viewer.html",
        "viewer.js",
        "toolbar.js",
        "security.js",
        "security.html",
        "calculator.js",
        "calculator.html",
        "endExam.js",
        "end_exam.html",
        "focus_lost.html",
        "monitor.html",
        "monitor.js",
        "loadingbar.html",
        "loadingbar.js",
        "early_exit.html",
        "earlyExit.js",
        "essayBlock.js",
        "_locales/de/messages.json",
        "_locales/en/messages.json",
        "_locales/es/messages.json",
        "_locales/fr/messages.json",
        "_locales/it/messages.json",
        "_locales/pt/messages.json",
        "/assets/images/close-tab.png",
        "/assets/images/calculator-light.png",
        "/assets/images/x-square.png",
        "/assets/images/recording-icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}